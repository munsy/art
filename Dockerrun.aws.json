{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "art-web",
      "image": "art:ui",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    },
    {
      "name": "art-api",
      "image": "art:api",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 443,
          "containerPort": 5000
        }
      ]
    }
  ]
}